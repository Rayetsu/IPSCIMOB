@{
    if (!this.User.Identity.IsAuthenticated)
    {
        Layout = "_Layout";
    }
    else if (this.User.IsInRole("Aluno") || this.User.IsInRole("Funcionário"))
    {
        Layout = "_LayoutAlunoFuncionario";
    }
    else if (this.User.IsInRole("CIMOB"))
    {
        Layout = "_LayoutCIMOB";
    }
}


@*@{
    Layout = "_Layout";
}*@
