@model IPSCIMOB.Models.CandidaturaModel
@{
    ViewData["Title"] = "FinalCandidatura";
    Layout = "_LayoutCandidatura";
    
}

<div align="center">
    <h2>Candidatura Programa de Mobilidade: @ViewBag.NomePrograma </h2>
    <h5>Passo 5 de 5</h5>
</div>

<div>
    <h4>Candidatura ao Programa de Mobilidade:</h4>
    <h4 class="text-danger">@ViewBag.NomePrograma</h4>
    <h4>Instituição: </h4>
    <h4 class="text-danger">@Model.InstituicaoNome</h4>
    <h4>País: </h4>
    <h4 class="text-danger">@Model.InstituicaoPais</h4>
    <h4>Cidade: </h4>
    <h4 class="text-danger">@Model.InstituicaoCidade</h4>
    <h4>Semestre: </h4>
    <h4 class="text-danger">@Model.Semestre</h4>
    <br />

    <h4>Estado da sua Candidatura: </h4>
    <h4 class="text-danger"> @Model.Estado</h4>
    <h4>Estado da Bolsa: </h4>
    <h4 class="text-danger">@Model.EstadoBolsa</h4>

    <br/>
    <h4>Data Inicio Candidatura: </h4>
    <h4 class="text-danger"> @Model.DataInicioCandidatura</h4>
    <h4>Data Fim Candidatura: </h4>
    <h4 class="text-danger">@Model.DataFimCandidatura</h4>
</div>

